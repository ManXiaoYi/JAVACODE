package com.my.tree;

import java.util.Comparator;

public class AVLTree<E> extends BST<E> {
	public AVLTree() {
		this(null);
	}

	public AVLTree(Comparator<E> comparator) {
		super(comparator);
	}
	
	@Override
	protected void afterAdd(Node<E> node) {
		// node的父节点 不为null时 循环
		while ((node = node.parent) != null) {
			
		}
		
		
	}
}
